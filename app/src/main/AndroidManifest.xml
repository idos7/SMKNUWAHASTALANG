        android:supportsRtl="true"
        android:theme="@style/Theme.SMKNUWAHASTALANG"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activity.VerifikasiPasswordActivity"
            android:label="@string/title_activity_verifikasi_password">
